@model IEnumerable<Web_DoChoi.Models.DanhMucModel>

<div class="dropdown-menu m-0 bg-secondary rounded-0">
    <a asp-action="TimKiemSP" asp-controller="Collection" class="dropdown-item"> Tất Cả Sản Phẩm</a>
    @foreach(var dm in Model)
    {
        <a asp-action="TimKiemSP" asp-controller="Collection" class="dropdown-item" asp-route-danhMucId="@dm.PK_DanhMuc">@dm.TenDanhMuc</a>
    }
    
</div>